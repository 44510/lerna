{
  "name": "@lerna/exec",
  "version": "6.4.0",
  "description": "Execute an arbitrary command in each package",
  "keywords": [
    "lerna",
    "command"
  ],
  "homepage": "https://lerna.js.org",
  "license": "MIT",
  "author": {
    "name": "Daniel Stockman",
